-- Nh·∫≠p S·ªë Mana
shared.THRESHOLD = 50
Tabs.Main:AddInput("ManaToReset", {
    Title = "Enter Mana To Reset",
    Default = "50",
    Placeholder = "50",
    Numeric = true,
    Callback = function(value)
        shared.THRESHOLD = tonumber(value) or 50
    end
})

-- Auto Check Mana (Ch·∫Øc ch·∫Øn kh√¥ng l·ªói)
do
    local manaConnection = nil
    local charAddedConn = nil
    local currentHumanoid = nil
    local progressStamina = nil
    local lowManaCount = 0
    local lastLowManaTime = 0
    local canAct = false

    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local player = Players.LocalPlayer

    -- H√†m ki·ªÉm tra mana m·ªói frame (d√πng l·∫°i sau reconnect)
    local function manaCheckFunction()
        if not shared.autoCheckMana or not canAct then return end
        if not currentHumanoid or not progressStamina then return end

        local manaText = progressStamina.Text or ""
        local currentMana = tonumber(manaText:match("(%d+)%s*/%s*%d+"))
        local now = tick()

        if currentMana and currentMana > 0 and currentMana < shared.THRESHOLD and currentHumanoid.Health > 0 and (now - lastLowManaTime >= 10) then
            lowManaCount += 1
            lastLowManaTime = now
            print("üß™ [ManaCheck] Mana th·∫•p l·∫ßn:", lowManaCount)
        elseif currentMana and currentMana >= shared.THRESHOLD then
            lowManaCount = 0
        end

        if lowManaCount >= 5 and currentHumanoid.Health > 0 then
            print("üü• [ManaCheck] Reset v√¨ mana th·∫•p!")
            currentHumanoid.Health = 0
            lowManaCount = 0
            canAct = false

            -- Ng·∫Øt ki·ªÉm tra t·∫°m th·ªùi
            if manaConnection then
                manaConnection:Disconnect()
                manaConnection = nil
            end

            task.delay(20, function()
                canAct = true
                if shared.autoCheckMana then
                    manaConnection = RunService.RenderStepped:Connect(manaCheckFunction)
                end
            end)
        end
    end

    -- H√†m g·ªçi m·ªói khi nh√¢n v·∫≠t m·ªõi xu·∫•t hi·ªán
    local function setupCharacter(char)
        lowManaCount = 0
        lastLowManaTime = 0
        canAct = false
        currentHumanoid = nil
        progressStamina = nil

        task.spawn(function()
            currentHumanoid = char:WaitForChild("Humanoid", 5)
            local gui = player:WaitForChild("PlayerGui")

            local ok, ui = pcall(function()
                return gui:WaitForChild("UI", 5):WaitForChild("HUD"):WaitForChild("Bars"):WaitForChild("ProgressStamina")
            end)

            if ok then
                progressStamina = ui
                print("‚úÖ [ManaCheck] T√¨m th·∫•y ProgressStamina.")
            else
                progressStamina = nil
                warn("‚ö†Ô∏è [ManaCheck] Kh√¥ng t√¨m th·∫•y ProgressStamina.")
            end
        end)

        -- Cho ph√©p ki·ªÉm tra sau 20s
        task.delay(20, function()
            canAct = true
        end)
    end

    -- Toggle trong UI
    Tabs.Main:AddToggle("AutoCheckManaToggle", {
        Title = "Auto Reset If Mana Is Low",
        Default = false,
        Callback = function(enabled)
            shared.autoCheckMana = enabled

            -- D·ªçn d·∫πp k·∫øt n·ªëi c≈©
            if manaConnection then
                manaConnection:Disconnect()
                manaConnection = nil
            end

            if charAddedConn then
                charAddedConn:Disconnect()
                charAddedConn = nil
            end

            if not enabled then return end

            -- N·∫øu nh√¢n v·∫≠t ƒëang t·ªìn t·∫°i th√¨ setup lu√¥n
            if player.Character then
                setupCharacter(player.Character)
            end

            -- G·∫Øn l·∫°i m·ªói l·∫ßn nh√¢n v·∫≠t m·ªõi
            charAddedConn = player.CharacterAdded:Connect(setupCharacter)

            -- B·∫Øt ƒë·∫ßu theo d√µi
            manaConnection = RunService.RenderStepped:Connect(manaCheckFunction)
        end
    })
end

