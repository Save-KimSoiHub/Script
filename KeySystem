local HttpService = game:GetService("HttpService")
local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local userKey = getgenv().Key or "0f5bf14d51b1f13bd5128d704"

local apiResponse = syn and syn.request or http_request or request

local res = apiResponse({
    Url = "https://api.pandadevelopment.net/v1/key-system/verify",
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json",
        ["Authorization"] = "Njb6LEi6Fn3JQOxgKPQeJ9ux2Mhb4f3R" -- Vanguard API Token của bạn
    },
    Body = HttpService:JSONEncode({
        key = userKey,
        service = "KimSoiHub",
        hwid = hwid
    })
})

local data = HttpService:JSONDecode(res.Body)

if data.valid == true then
    print("✅ Key hợp lệ! Đang tải script...")
    loadstring(game:HttpGet("https://pandadevelopment.net/virtual/file/7e535feb82359d7f"))()
else
    print("❌ Key không hợp lệ!", data.message or "")
end
