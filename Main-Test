    Title = "Teleport To Selected Player",
    Callback = function()
        if selectedPlayerUsername then
            teleportToPlayer(selectedPlayerUsername)
        else
            warn("B·∫°n ch∆∞a ch·ªçn ng∆∞·ªùi ch∆°i ƒë·ªÉ teleport.")
        end
    end
})

